(data) => {this.userData = data ;})\n        },\n        \n    },\n    created(){\n        this.fetchData()\n    },\n    computed: {\n            postsLink() {\n                return `/user/${this.$route.params.id}/posts` \n            },\n            nextUserLink(){\n             return `/user/${parseInt(this.$route.params.id,10) +1}`\n            },\n            previousUser(){\n                return `/user/${parseInt(this.$route.params.id,10)-1}`\n            }\n    },\n    watch: {\n        '$route' : 'fetchData'\n    },\n        \n}\n</script>"]}]}